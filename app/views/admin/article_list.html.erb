<div class="col-sm-12">
  <h1 class="blog-title header-part">Articles List</h1>
</div>
<div class="col-sm-12">
  <table class="table table-bordered">
    <thead>
    <tr>
      <th>Title</th>
      <th>Content</th>
      <th>Image Name</th>
      <th>Video Url</th>
      <th>User</th>
      <th>Actions</th>
    </tr>
    </thead>
    <tbody>
    <% @articles.each do |article| %>
        <tr>
          <td><%= article.title %></td>
          <td><%= article.title %></td>
          <td><%= article.title %></td>
          <td><%= article.title %></td>
          <td><%= article.title %></td>
          <td>
            <button type="button" class="btn btn-primary" aria-label="Edit">Edit</button>
            <button type="button" class="btn btn-danger" aria-label="Delete">Delete</button>
          </td>
        </tr>
    <% end %>
    </tbody>
  </table>
</div>
